{
    "title": "Law-Book",
    "author": "Oreomeow",
    "description": "中国常用法律查询手册 | Law-Book",
    "generator": "site",
    "plugins": [
        "-lunr",
        "-search",
        "-sharing",
        "anchor-navigation-ex",
        "chapter-fold",
        "favicon-absolute",
        "github",
        "hide-element",
        "insert-logo",
        "search-pro",
        "sharing-plus",
        "splitter",
        "tbfed-pagefooter",
        "theme-comscore"
    ],
    "pluginsConfig": {
        "anchor-navigation-ex": { "showLevel": false },
        "favicon-absolute": {
            "favicon": "images/favicon.ico",
            "appleTouchIconPrecomposed152": "images/apple-touch-icon-precomposed-152.png"
        },
        "github": { "url": "https://github.com/Oreomeow/Law-Book" },
        "hide-element": {
            "elements": [".gitbook-link"]
        },
        "insert-logo": {
            "url": "images/law-book.png",
            "style": "background: none; max-height: 30px; min-height: 30px"
        },
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": false,
            "pocket": true,
            "qq": false,
            "qzone": false,
            "twitter": false,
            "weibo": false,
            "all": ["douban", "facebook", "google", "instapaper", "linkedin", "twitter", "weibo", "messenger", "qq", "qzone", "viber", "whatsapp"]
        },
        "tbfed-pagefooter": {
            "copyright": "根据《中华人民共和国著作权法》第五条，本作品不适用于该法。如不受其他法律、法规保护，本作品在中国大陆和其他地区属于公有领域。不适用于《中华人民共和国著作权法》的作品包括：<br>（一）法律、法规，国家机关的决议、决定、命令和其他具有立法、行政、司法性质的文件，及其官方正式译文；<br>（二）单纯事实消息；<br>（三）历法、通用数表、通用表格和公式。<br>法律内容来源于国家法律法规数据库，该项目仅做整合和搜索等功能，如果您在使用过程中发现部分法条有误，或不完整，请联系开发者进行修改。",
            "modify_label": "该手册修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}
